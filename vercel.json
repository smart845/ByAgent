{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/src/(.*)", "destination": "/src/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
